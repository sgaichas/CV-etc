	file = {Goethel et al. - 2019 - Recent advances in management strategy evaluation.pdf:/Users/sgaichas/Zotero/storage/6ZAW3QWZ/Goethel et al. - 2019 - Recent advances in management strategy evaluation.pdf:application/pdf}
}

@article{tommasi_managing_2017,
